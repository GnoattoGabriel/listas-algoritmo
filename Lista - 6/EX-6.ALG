Algoritmo "ex-6"
Var
   palavra1, palavra2, concatenada: caractere
Inicio
   Escreva("Digite a primeira palavra: ")
   Leia(palavra1)

   Escreva("Digite a segunda palavra: ")
   Leia(palavra2)

   concatenada := palavra1 + palavra2

   Escreva("A palavra concatenada é: ", concatenada)
Fimalgoritmo
